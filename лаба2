Главная.java Арифметические операции с присваиванием
публичный класс Main (

public static void main (String []args){ 

/*

		+=
		*=
		/=
		-=
		%=

 x += y; = > x = x + y;
*/

double а = 9;
двойной b = 10;
двойной c = 5;
а += b;
а %= 4;
а /= (с + 1)/2 + а;
а -= 0.5;
а -= 0.5;
а -= 0.5;
а -= 0.5;
а -= 0.5;
а -= 0.5;
Система.из.println (“a: "+ a);
}
}
	

	





