 public static void main (String[] args) {

		/*

 тип имя_mac[] = new тип[РАЗМЕР];
 тип []имя_mac = new тип[РАЗМЕР];
 тип[] имя_mac = new тип[РАЗМЕР];

		*/
 int mas[] = новый int[10];

 для(int i = 0; i длина; i++){
 mas[i] = (int)Math.раунд (математика.случайный ()*100);
	System.out.println (mas[i] +  ” “);
}
Система.из.println();

int min = mas[0];
int n = 32;
ибо (int i = 1; I длина; i++){
 если (мин ]){
 min = mas[i];
	}
}
