Главная.java Оператор цикла for
публичный класс Main {
	
 public static void main (String[] args) {

		/*

 for (инициализация; условие; шаг) {
 //Тело цикла
		}

		*/

 int count = -1;
int sum = 0;
для (int i = -10; i ){
 если ((i % 3) = = 0){
 Система.из.println(i);
 рассчитывать++;
		}
 если (((кол-во % 2) == 0) && ((i %3) = = 0)){
 сумма += i;

		}
	}

Система.из.println ("sum:" + sum);
}
}




















